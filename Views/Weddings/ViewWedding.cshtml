@model Wedding
<div class="row center-align">
    <h3 id="fittext">@Model.WedderOne and @Model.WedderTwo's Wedding</h3>
    <a class="btn pink-on-white hoverable" href="/logout"><i class="material-icons right">near_me</i>Log Out</a>
    <a class="btn pink-on-white hoverable ml-10" href="/Dashboard"><i class="material-icons right">home</i>Dashboard</a>
</div>
<div class="divider row"></div>
<div class="row">
    <div class="col s12 l4">
        <p class="font-20">Date:</p>
        <p class="font-20" style="margin-left: 50px;">@Model.Date.ToString("MMMM dd yyyy h:mm tt")</p>
        <p class="font-20">Guests:</p>
        <ul>
        @foreach (RSVP rsvp in Model.RSVPs)
        {
            <li class="font-20" style="margin-left: 50px;">- @rsvp.user.FirstName @rsvp.user.LastName</li>
        }
        </ul>
    </div>
    <div class="col s12 l7 offset-l1">
        <iframe width="100%" height="450" frameborder="10" style="border:5; margin-top: 20px;"
            src="https://www.google.com/maps/embed/v1/place?key=AIzaSyDhc_4rfCm9kufSuafhtXU56C_gaeayl1o&q=@Model.Address" allowfullscreen>
        </iframe>
    </div>
</div>